{
  "name": "angular-storybook-library",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "start": "npm run docs:json && start-storybook -p 6006",
    "check": "npm run lint && npm run test",
    "test": "ng test",
    "test:watch": "ng test --watch",
    "test:debug": "node --inspect-brk node_modules/jest/bin/jest.js --run-in-band",
    "lint": "npm-run-all -s -c lint:scss prettier lint:ts",
    "lint:fix": "npm-run-all -s -c lint:scss:fix prettier:fix lint:ts:fix",
    "lint:ts": "ng lint",
    "lint:ts:fix": "ng lint --fix",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint:scss:fix": "stylelint \"src/**/*.scss\" --fix",
    "prettier": "prettier --check \"src/**/*.{scss,html}\"",
    "prettier:fix": "prettier --write \"src/**/*.{scss,html}\"",
    "docs": "compodoc -p ./tsconfig.json src -d build/documentation",
    "docs:json": "compodoc -p ./tsconfig.json src -e json -d build/documentation",
    "clean": "rimraf dist/ build/ *.tgz",
    "build": "ng build",
    "build:watch": "ng build --watch --configuration development",
    "build:storybook": "npm run docs:json && build-storybook -o build/storybook",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prepublish": "npm run check && npm run build",
    "publish": "npm publish dist/"
  },
  "dependencies": {
    "tslib": "2.3.0"
  },
  "peerDependencies": {
    "@angular/common": "~13.2.0",
    "@angular/core": "~13.2.0"
  },
  "devDependencies": {
    "@angular-builders/jest": "^13.0.3",
    "@angular-devkit/build-angular": "~13.2.4",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/animations": "~13.2.0",
    "@angular/cli": "~13.2.4",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/compiler-cli": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/forms": "~13.2.0",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "@babel/core": "^7.17.5",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@compodoc/compodoc": "^1.1.18",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/angular": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/testing-library": "0.0.9",
    "@types/jest": "^27.4.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "babel-loader": "^8.2.3",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^24.7.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^4.3.8",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.4",
    "ng-packagr": "^13.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "rxjs": "~7.5.0",
    "stylelint": "^14.5.1",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "ts-jest": "^27.1.3",
    "typescript": "~4.5.2",
    "zone.js": "~0.11.4"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": ">=14.18.1",
    "npm": ">=6.14.15"
  }
}
